{
  "name": "eco.mio | Hiring Challenge",
  "description": "Good luck. You got this!",
  "version": "0.0.1",
  "manifest_version": 3,
  "web_accessible_resources": [
    {
      "resources": ["images/app/*.png", "js/index.css"],
      "matches": ["*://*/*"]
    }
  ],
  "action": {
    "default_popup": "js/index.html"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/contentScript.js"]
    }
  ]
}
